<!DOCTYPE html>
<html>
  <head>
    <title>Student Registration Form</title>
    <style>
      /* CSS styles for the form */
      body {
        font-family: Arial, sans-serif;
      }

      label {
        display: block;
        margin-bottom: 5px;
      }

      input[type="text"],
      input[type="email"],
      select {
        width: 100%;
        padding: 10px;
        border: 1px solid #ccc;
        border-radius: 4px;
        box-sizing: border-box;
        margin-bottom: 10px;
      }

      input[type="submit"],
      input[type="reset"] {
        background-color: #4caf50;
        color: #fff;
        padding: 10px 15px;
        border: none;
        border-radius: 4px;
        cursor: pointer;
      }

      input[type="submit"]:hover,
      input[type="reset"]:hover {
        background-color: #3e8e41;
      }

      .error {
        color: red;
        font-size: 0.8em;
        margin-top: 5px;
      }
    </style>
    <script>
      function validateForm() {
        // Get form elements
        var rollno = document.forms["registration"]["rollno"].value;
        var name = document.forms["registration"]["name"].value;
        var email = document.forms["registration"]["email"].value;
        var dob = document.forms["registration"]["dob"].value;
        var gender = document.forms["registration"]["gender"].value;
        var courses = document.forms["registration"]["courses"].value;
        var terms = document.forms["registration"]["terms"].checked;

        // Roll no validation
        var rollnoRegExp = /^\d{7}$/;
        if (!rollnoRegExp.test(rollno)) {
          document.getElementById("rollno-error").innerHTML =
            "Roll number must be a 7-digit numeric value";
          return false;
        } else {
          document.getElementById("rollno-error").innerHTML = "";
        }

        // Name validation
        var nameRegExp = /^[a-zA-Z ]+$/;
        if (!nameRegExp.test(name)) {
          document.getElementById("name-error").innerHTML =
            "Name must be an alphabetical value";
          return false;
        } else {
          document.getElementById("name-error").innerHTML = "";
        }

        // Email validation
        var emailRegExp = /^\S+@\S+\.\S+$/;
        if (!emailRegExp.test(email)) {
          document.getElementById("email-error").innerHTML =
            "Please enter a valid email address";
          return false;
        } else {
          document.getElementById("email-error").innerHTML = "";
        }

        // Date of birth validation
        if (dob == "") {
          document.getElementById("dob-error").innerHTML =
            "Please enter your date of birth";
          return false;
        } else {
          document.getElementById("dob-error").innerHTML = "";
        }

        // Gender validation
        if (gender == "") {
          document.getElementById("gender-error").innerHTML =
            "Please select your gender";
          return false;
        } else {
          document.getElementById("gender-error").innerHTML = "";
        }

        // Courses validation
        if (courses == "") {
          document.getElementById("courses-error").innerHTML =
            "Please select at least one course";
          return false;
        } else {
          document.getElementById("courses-error").innerHTML = "";
        }

        // Terms and conditions validation
        if (!terms) {
          document.getElementById("terms-error").innerHTML =
            "You must agree to the terms and conditions";
            
